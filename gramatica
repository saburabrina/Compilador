PROGRAMA -> DECVAR PROTEFUNCOES FESCOPO
DECVAR -> VARIDS . DECVAR
DECVAR -> VARVET . DECVAR
DECVAR -> ''
VARID -> TIPO : ID
VARIDS -> TIPO : LISTAIDS
VARVET -> VET TIPO : ID VALOR
VARVETPASS -> VET TIPO : ID
LISTAIDS -> ID SEMICOMMAID
SEMICOMMAID -> ; ID SEMICOMMAID
SEMICOMMAID -> ''
PROTEFUNCOES -> CABECALHOFUNCAO PF
PROTEFUNCOES -> ''
CABECALHOFUNCAO -> FUNCAO ID ( LISTAVAR ) : RETORNO
LISTAVAR -> VARX SEMICOMMAVAR
LISTAVAR -> '' 
VARX -> VARID
VARX -> VARVETPASS
SEMICOMMAVAR -> ; VARX SEMICOMMAVAR
SEMICOMMAVAR -> ''
RETORNO -> TIPO
RETORNO -> NULO
PF -> . PROTEFUNCOES
PF -> FESCOPO REPORFIM
FESCOPO -> VAR DECVAR INICIO CODIGO FIM
CODIGO -> ''
CODIGO -> ID MEIOT1 CODIGO
CODIGO -> DC CODIGO
CODIGO -> LOOP CODIGO
CODIGO -> LEITURA CODIGO
CODIGO -> ESCRITA CODIGO
CODIGO -> PARE . CODIGO
CODIGO -> CMDRECEBA CODIGO
MEIOT1 -> ATR
MEIOT1 -> CF .
ATR -> VECTORORNOT <- EXPA .
EXPA -> SUBEXPA EXP1
EXP1 -> OP1 SUBEXPA EXP1
EXP1 -> ''
OP1 -> +
OP1 -> -
SUBEXPA -> DEEPEXPA EXP2 
EXP2 -> OP2 DEEPEXPA EXP2
EXP2 -> ''
DEEPEXPA -> DEEPEREXPA EXP3
EXP3 -> ** DEEPEREXPA EXP3
EXP3 -> ''
DEEPEREXPA -> - EXP4
DEEPEREXPA -> EXP4
EXP4 -> ( EXPA )
EXP4 -> VALOR
EXP4 -> ID MEIOT2
MEIOT2 -> CF
MEIOT2 -> VECTORORNOT
CF -> ( LISTAELEM )
LISTAELEM -> EXPA SEMICOMMAELEM
LISTAELEM -> ''
SEMICOMMAELEM -> ; EXPA SEMICOMMAELEM
SEMICOMMAELEM -> ''
DC -> SE EXPRC INICIO CODIGO FIM ELSE
EXPRC -> MT
MT -> NR4 NR1
EXPR -> EXPA OPR EXPA
NR1 -> | NR2A NR1
NR1 -> NR2B NR1
NR1 -> ''
NR2A -> NR3
NR2B -> & NR3
NR3 -> ! NR4
NR3 -> NR4
NR4 -> ( MT )
NR4 -> EXPR
ELSE -> SENAO INICIO CODIGO FIM
ELSE -> ''
LOOP -> ENQUANTO EXPRC INICIO CODIGO FIM
LOOP -> PARA ID DE EXPA ATEH EXPA INICIO CODIGO FIM
LEITURA -> LEIA LISTAIDSCV .
ESCRITA -> ESCREVA LISTAA .
LISTAIDSCV -> ID VECTORORNOT SEMICOMMAIDCV
SEMICOMMAIDCV -> ; ID VECTORORNOT SEMICOMMAIDCV
SEMICOMMAIDCV -> ''
LISTAA -> ELEM SEMICOMMAA
SEMICOMMAA -> ; ELEM SEMICOMMAA
SEMICOMMAA -> ''
ELEM -> EXPA
ELEM -> TEXTO
CMDRECEBA -> RECEBA RECEBACONTEUDO .
VECTORORNOT -> [ EXPA ]
VECTORORNOT -> ''
RECEBACONTEUDO -> ''
RECEBACONTEUDO -> ( EXPA )
REPORFIM -> CABECALHOFUNCAO FESCOPO REPORFIM
REPORFIM -> ''