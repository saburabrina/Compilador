PROGRAMA -> CONTEUDO

CONTEUDO -> DECVAR PROTFUNCOES FUNCOES VAR DECVAR INICIO CODIGO FIM

DECVAR-> TIPO : LISTAIDS . DECVAR
DECVAR -> VET TIPO : ID INT . DECVAR
DECVAR -> ''

TIPO-> INTEIRO
TIPO-> REAL

VALOR -> INT
VALOR -> FLOAT

MINUSORNOT -> -
MINUSORNOT -> ''

LISTAIDS -> ID SEMICOMMAID

SEMICOMMAID -> ; ID
SEMICOMMAID -> ''

ATR -> ID VECTORORNOT <- EXPA .

VECTORORNOT -> [ EXPA ]
VECTORORNOT -> ''

EXPA-> OPERANDO1 EXP1

OPERANDO1 -> VALOR
OPERANDO1 -> ID VECTORORNOT
OPERANDO1 -> SUBEXPA

EXP1 -> OP1 OPERANDO1 EXP1
EXP1 -> ''

OP1 -> +
OP1 -> -

SUBEXPA -> OPERANDO2 EXP2 

OPERANDO2 -> VALOR
OPERANDO2 -> ID VECTORORNOT
OPERANDO2 -> DEEPEXPA

EXP2 -> OP2 OPERANDO2 EXP2
EXP2 -> ''

OP2 -> *
OP2 -> /
OP2 -> %

DEEPEXPA -> OPERANDO3 EXP3

OPERANDO3 -> VALOR
OPERANDO3 -> ID VECTORORNOT
OPERANDO3 -> DEEPEREXPA

EXP3 -> ** OPERANDO3 EXP3
EXP3 -> ''
