PROGRAMA -> CONTEUDO

CONTEUDO -> DECVAR PROTFUNCOES FUNCOES VAR DECVAR INICIO CODIGO FIM

DECVAR -> VARIAVEL . DECVAR
DECVAR -> VARIAVEL . DECVAR
DECVAR -> ''

VARIAVEL -> TIPO : LISTAIDS
VARIAVEL -> VET TIPO : ID INT

TIPO -> INTEIRO
TIPO -> REAL

VALOR -> INT
VALOR -> FLOAT

MINUSORNOT -> -
MINUSORNOT -> ''

LISTAIDS -> ID SEMICOMMAID

SEMICOMMAID -> ; ID SEMICOMMAID
SEMICOMMAID -> ''

LISTAIDSCV -> ID VECTORORNOT SEMICOMMAIDCV

SEMICOMMAIDCV -> ; ID VECTORORNOT SEMICOMMAIDCV
SEMICOMMAIDCV -> ''

LISTAELEM -> ID VECTORORNOT SEMICOMMAELEM
LISTAELEM -> EXPA SEMICOMMAELEM
LISTAELEM -> VALOR SEMICOMMAELEM

SEMICOMMAELEM -> ; ELEM SEMICOMMAELEM
SEMICOMMAELEM -> ''

ELEM -> ID VECTORORNOT
ELEM -> EXPA
ELEM -> VALOR

ATR -> ID VECTORORNOT <- EXPA .

VECTORORNOT -> [ EXPA ]
VECTORORNOT -> ''

EXPA -> OPERANDO1 EXP1
EXPR -> EXPA OPR EXPA
EXPRC -> EXPR NR1

NR1 -> | NR2
NR1 -> NR2

NR2 -> & NR3
NR2 -> NR3

NR3 -> ! NR4
NR3 -> NR4
NR3 -> ''

NR4 -> ( EXPRC )
NR4 -> EXPRC

OPR -> <
OPR -> >
OPR -> <=
OPR -> >=
OPR -> = 
OPR -> <>

OPERANDO1 -> VALOR
OPERANDO1 -> ID VECTORORNOT
OPERANDO1 -> SUBEXPA

EXP1 -> OP1 OPERANDO1 EXP1
EXP1 -> ''

OP1 -> +
OP1 -> -

SUBEXPA -> OPERANDO2 EXP2 

OPERANDO2 -> VALOR
OPERANDO2 -> ID VECTORORNOT
OPERANDO2 -> DEEPEXPA

EXP2 -> OP2 OPERANDO2 EXP2
EXP2 -> ''

OP2 -> *
OP2 -> /
OP2 -> %

DEEPEXPA -> OPERANDO3 EXP3

OPERANDO3 -> VALOR
OPERANDO3 -> ID VECTORORNOT
OPERANDO3 -> DEEPEREXPA

EXP3 -> ** OPERANDO3 EXP3
EXP3 -> ''

DEEPEREXPA -> ( EXPA )

DC -> SE EXPRC INICIO CODIGO FIM ELSE

ELSE -> SENAO INICIO CODIGO FIM
ELSE -> ''

LOOP -> ENQUANTO EXPRC INICIO CODIGO FIM
LOOP -> PARA ID DE EXPA ATEH EXPA INICIO CODIGO FIM

LEITURA -> LEIA LISTAIDSCV .

ESCRITA -> ESCREVA LISTAELEM .

PROTFUNCOES -> FUNCAO ID ( LISTAVAR ) : RETORNO .

RETORNO -> TIPO
RETORNO -> NULO

LISTAVAR-> VARIAVEL SEMICOMMAVAR

SEMICOMMAVAR -> ; VARIAVEL SEMICOMMAVAR
SEMICOMMAVAR -> ''

FUNCOES -> FUNCAO ID ( LISTAVAR ) : RETORNO VAR DECVAR INICIO CODIGO FIM