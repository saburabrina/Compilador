PROGRAMA -> CONTEUDO

CONTEUDO -> DECVAR PROTFUNCOES FUNCOES VAR DECVAR INICIO CODIGO FIM

DECVAR-> TIPO : LISTAIDS . DECVAR
DECVAR -> VET TIPO : ID POSINT . DECVAR
DECVAR -> ''

TIPO-> INTEIRO
TIPO-> REAL

VALOR -> INT
VALOR -> FLOAT

INT -> MINUSORNOT POSINT
FLOAT -> MINUSORNOT POSFLOAT

MINUSORNOT -> -
MINUSORNOT -> ''

LISTAIDS -> ID SEMICOMMAID

SEMICOMMAID -> ; ID
SEMICOMMAID -> ''

ATR -> ID VECTORORNOT <- EXPARITMETICA .

VECTORORNOT -> [ POSEXPARITMETICA ]
VECTORORNOT -> ''

EXPARITMETICA -> MINUSORNOT POSEXPARITMETICA

POSEXPARITMETICA -> VALOR EXP

EXP -> OP VALOR EXP
EXP -> ''
